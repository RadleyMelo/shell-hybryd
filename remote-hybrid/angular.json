{
  "$schema": "https://json.schemastore.org/angular.json",
  "version": 1,
  "projects": {
    "remote-hybrid": {
      "projectType": "application",
      "root": "",
      "sourceRoot": "src",
      "architect": {
        "build": {
          "builder": "ngx-build-plus:browser",
          "options": {
            "outputPath": "dist/remote-hybrid",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [],
            "scripts": [],
            "tsConfig": "tsconfig.app.json",
            "extraWebpackConfig": "webpack.config.cjs"
          },
          "configurations": {
            "development": {
              "extractCss": true
            },
            "mf": {
              "outputHashing": "none",
              "vendorChunk": true,
              "commonChunk": true,
              "namedChunks": true
            }
          }
        },
        "build-wc": {
          "builder": "ngx-build-plus:browser",
          "options": {
            "outputPath": "dist/remote-hybrid",
            "index": "src/index.html",
            "main": "src/main.wc.ts",
            "polyfills": [
              "zone.js"
            ],
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [],
            "scripts": [],
            "tsConfig": "tsconfig.app.json",
            "extraWebpackConfig": "extra-webpack.wc.cjs",
            "outputHashing": "none"
          },
          "configurations": {
            "development": {
              "extractCss": true
            }
          }
        },
        "serve": {
          "builder": "ngx-build-plus:dev-server",
          "configurations": {
            "mf": {
              "browserTarget": "remote-hybrid:build:mf",
              "extraWebpackConfig": "webpack.config.cjs"
            },
            "wc": {
              "browserTarget": "remote-hybrid:build-wc"
            }
          },
          "options": {
            "browserTarget": "remote-hybrid:build"
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "83aee6a2-5096-43ae-b658-e3720f859072"
  }
}
